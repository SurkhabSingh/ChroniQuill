<h3 class="nav-items">Login</h3>

<form action="/admin" method="POST">
  <label class="nav-items"><b>Username</b></label>
  <input type="text" placeholder="Enter Username" name="username" />
  <label class="nav-items" for="password"><b>Password</b></label>
  <input type="text" placeholder="Enter Password" name="password" />
  <input type="submit" value="Login" class="btn" />
</form>

<h3 class="nav-items">Register</h3>

<form action="/register" method="POST">
  <label class="nav-items for="username"><b>Username</b></label>
  <input type="text" placeholder="Enter Username" name="username" />
  <label class="nav-items for="password"><b>Password</b></label>
  <input type="text" placeholder="Enter Password" name="password" />
  <input type="submit" value="Register" class="btn" />
</form>
