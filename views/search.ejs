<div class="author">
  <h1 class="author-heading">Search Results</h1>
</div>

<section class="articles">
  <ul class="articles-ul">
    <% data.forEach(post => { %>

    <li>
      <a href="/post/<%= post._id %>">
        <span><%= post.title %></span>
        <span class="articles-listdate"
          ><%= post.createdAt.toDateString() %></span
        >
      </a>
    </li>
    <% }) %>
  </ul>
</section>
